<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.dashboard.DashboardFragment">


    <TextView
        android:id="@+id/xValue"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="14dp"
        app:layout_constraintEnd_toStartOf="@+id/btnStart"
        app:layout_constraintStart_toStartOf="@+id/xGyroValue"
        app:layout_constraintTop_toBottomOf="@+id/btnStart" />

    <TextView
        android:id="@+id/yValue"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnStart" />

    <TextView
        android:id="@+id/zValue"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toTopOf="@+id/zGyroValue"
        app:layout_constraintEnd_toEndOf="@+id/zGyroValue"
        app:layout_constraintStart_toStartOf="@+id/zMagnoValue" />

    <TextView
        android:id="@+id/xGyroValue"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBaseline_toBaselineOf="@+id/yGyroValue"
        app:layout_constraintEnd_toEndOf="@+id/xMagnoValue"
        app:layout_constraintStart_toStartOf="@+id/xMagnoValue" />

    <TextView
        android:id="@+id/yGyroValue"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        app:layout_constraintEnd_toEndOf="@+id/yMagnoValue"
        app:layout_constraintStart_toStartOf="@+id/yMagnoValue"
        app:layout_constraintTop_toBottomOf="@+id/yValue" />

    <TextView
        android:id="@+id/zGyroValue"
        android:layout_width="93dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/yGyroValue"
        app:layout_constraintStart_toStartOf="@+id/zMagnoValue"
        app:layout_constraintTop_toTopOf="@+id/yGyroValue" />

    <TextView
        android:id="@+id/xMagnoValue"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/yMagnoValue"
        app:layout_constraintEnd_toStartOf="@+id/yMagnoValue"
        app:layout_constraintStart_toStartOf="@+id/btnPothole" />

    <TextView
        android:id="@+id/yMagnoValue"
        android:layout_width="107dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="18dp"
        app:layout_constraintStart_toStartOf="@+id/buttonStartLocationUpdates"
        app:layout_constraintTop_toBottomOf="@+id/yGyroValue" />


    <TextView
        android:id="@+id/zMagnoValue"
        android:layout_width="93dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="37dp"
        app:layout_constraintEnd_toEndOf="@+id/buttonStopLocationUpdates"
        app:layout_constraintStart_toEndOf="@+id/yMagnoValue"
        app:layout_constraintTop_toBottomOf="@+id/zGyroValue" />

    <Button
        android:id="@+id/btnPothole"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="4dp"
        android:layout_marginTop="25dp"
        android:text="mark pothole"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvSpeed" />

    <Button
        android:id="@+id/btnHump"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="30dp"
        android:text="mark hump"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvSpeed" />

    <Button
        android:id="@+id/btnStart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="11dp"
        android:text="start driving"
        app:layout_constraintEnd_toStartOf="@+id/zValue"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonStartLocationUpdates"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="60dp"
        android:text="@string/start_location_updates"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/xMagnoValue" />

    <Button
        android:id="@+id/buttonStopLocationUpdates"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="1dp"
        android:text="@string/stop_location_updates"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonStartLocationUpdates"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/tvLatlong"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="73dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/tvSpeed"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tvSpeed"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="194dp"
        android:text="TextView"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>